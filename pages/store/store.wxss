page{  
  background: #f5f5f5;  
}  
/*总体主盒子*/  
.container { 
  width: 100%;  
  height: 100%;  
  background-color: #fff;  
  color: #939393; 
 
}
 /*左侧栏主盒子*/  
.nav_left{  
  /*设置行内块级元素（没使用定位）*/  
  position: absolute;
  top: 0;left: 0;
  width: 25%;  
  height: 100%;  
  /*主盒子设置背景色为灰色*/  
  background: #F6F6FA;  
  text-align: center;  
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
  /*每个高30px*/  
  height: 40px;  
  /*垂直居中*/  
  line-height: 40px;  
  /*再设上下padding增加高度，总高42px*/  
 
  /*只设下边线*/  
  border-bottom: 1px solid #dedede;  
  /*文字14px*/  
  font-size: 14px; 
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  /*背景色变成白色*/  
  background: white;  
  color: #f0145a; 
}
.bookImg{  
  /*被图片设置宽高*/  
  width: 80px;  
  height: 80px;  
  margin-top: 15px;  
}  
.bookName{  
  /*给text设成块级元素*/ 
  width: 80px;
  height: 35px;
  margin-top: 15px;  
  font-size: 14px;  
  color: black;
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: wrap;  
  text-overflow: ellipsis;  
  text-align: center;
}
.bookItem{
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}
.nav_right{
  display: flex;
  flex-direction: row;
  flex-flow: wrap;
  align-content:flex-start;
  margin-left: 207rpx;
  height: 1200rpx;
}
.nodata_text
{
  color: black;
  font-size: 14px;  
  text-align: center;  
}
.tabLow{
  position: fixed;
  bottom: 0rpx;
  width: 750rpx;
}